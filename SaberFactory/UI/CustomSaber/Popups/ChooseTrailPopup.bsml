<bg bg="round-rect-panel" border="" raycast="true">
    <vertical pad-bottom='4'>
        <sui.custom-list id='saber-list' title='Choose trail from saber' width='80' bg-color='#FFFFFF05'/>
        <horizontal>
            <sui.button text='Select' pref-width='20' on-click='click-select' normal-color='#00000080'/>
            <sui.button text='Original' pref-width='20' on-click='click-original' normal-color='#00000080'/>
            <sui.button text='Cancel' pref-width='20' on-click='click-cancel' normal-color='#00000080'/>
        </horizontal>
    </vertical>
</bg>