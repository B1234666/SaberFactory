<vertical spacing='2' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>

  <vertical pref-width='16' background='round-rect-panel' bg-color='#666666bb' pad='2'>
    <macro.for-each items='nav-buttons'>
      <sui.nav-button
        id='button'
        on-selected='selected'
        icon='~icon'
        hover-hint='~hover-hint'
        on-color='#fff'
        off-color='#ffffff60'/>
    </macro.for-each>

    <sui.nav-button
      icon='SaberFactory.Resources.Icons.cog.png'
      hover-hint='Set up Saber Factory'
      on-selected='clicked-settings'
      on-color='#fff'
      off-color='#ffffff60'>
      <text id='settings-notify-text' active='false' text='Update' font-size='3' align='TopGeoAligned' color='#5dc968'/>
    </sui.nav-button>
  </vertical>

  <vertical pref-width='16' background='round-rect-panel' bg-color='#666666bb' pad='2'>
    <sui.icon-button
      pref-width='15'
      preferred-height='15'
      on-click='clicked-exit'
      icon='#ExitButton'
      hover-hint='Exit this earths athmosphere'
      skew='0' show-line='false' normal-color='#00000000' hovered-color='#ff424280'>
    </sui.icon-button>
  </vertical>
	
</vertical>